<%- include("partials/head") %>
<link rel="stylesheet" href="/css/deleteAdmin.css">
</head>
<body>
    <main>
        <%- include("partials/nav") %>
        <section>
            <ul id="list-admins">
                <% admins.forEach(admin => {%>
                <li class="card-admin">
                    <h3>ID: <%- admin.id_admin %></h3>
                    <p>EMAIL: <%- admin.correo %></p>
                    <p>NOMBRE: <%- admin.nombre %></p>
                    <button class="deleteAdminButton" data-id_admin= <%- admin.id_admin %>>Eliminar admin</button>
                </li>
                <% }) %>
            </ul>
        </section>
    </main>
    <%- include("partials/footer") %>
    <script src="/js/functions_deleteAdmin.js" type="module"></script>
    <script src="https://kit.fontawesome.com/886f04578b.js" crossorigin="anonymous"></script>
</body>